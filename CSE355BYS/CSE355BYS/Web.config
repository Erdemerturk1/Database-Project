<?xml version="1.0" encoding="utf-8"?>
<configuration>

	<connectionStrings>
		<add name="conStr"
			 connectionString="Data Source=YOUR_SERVER;Initial Catalog=FurnicolorDB;Integrated Security=True;"
			 providerName="System.Data.SqlClient" />
	</connectionStrings>


	<system.web>
		<compilation debug="true" targetFramework="4.8" />
		<httpRuntime targetFramework="4.8" />
	</system.web>

	
	
	<system.webServer>
		<httpErrors errorMode="Detailed" existingResponse="PassThrough" />
	</system.webServer>


</configuration>
	